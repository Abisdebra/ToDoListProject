{
  "compilerOptions": {
      "target": "es2022",
       "lib": ["ES6", "DOM"],
      "module": "ES2022",
      "rootDir": "./src",
      "allowJs": true,
      "outDir": "./dist",
      "noEmitOnError": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipDefaultLibCheck": true,
      "skipLibCheck": true,
  }
}